        android:layout_height="70dp"
        android:background="#00FFBF00"
        android:text="@string/whiteSpace"
        android:textColor="#00FFFFFF"
        app:backgroundTint="#43FFBF00"
        app:layout_constraintBottom_toBottomOf="@+id/helptext_general"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/optionsimage_general"
        tools:ignore="TextContrastCheck" />

