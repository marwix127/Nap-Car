                <TextView
                    android:id="@+id/label9_infoVehiculos"
                    android:layout_width="160dp"
                    android:layout_height="wrap_content"
                    android:text="@string/Motor_vehiculo"
                    android:textColor="#000000"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
